@using BiofuelSouth.Models
@model Storage



<div class="row">
    <div id="UserSupplyStorageCost">
        <div class="col-xs-6">
          
                @Html.LabelFor(m => m.UserEstimatedCost, new { @class = "control-label" })<span class="required">Required</span>
                @Html.TextBoxFor(m => m.UserEstimatedCost, new { @class = "form-control" })
                @Html.ValidationMessageFor(m => m.UserEstimatedCost, "", new { @class = "text-danger" })
            
        </div>
    </div>
</div>


<div class="row">
	<div id="UserSupplyMaterialCost">
		<div class="col-xs-12">
			<br/>
			<strong>Please modify any of the cost below:</strong>
		</div>
			<div class="col-xs-6">

				@Html.LabelFor( m => m.TarpCost, new { @class = "control-label" } ) <span class="required">Required</span>
				@Html.TextBoxFor( m => m.TarpCost, new { @class = "form-control" } )
				@Html.ValidationMessageFor( m => m.TarpCost, "", new { @class = "text-danger" } )

			</div>
			<div class="col-xs-6">

				@Html.LabelFor( m => m.PalletCost, new { @class = "control-label" } ) <span class="required">Required</span>
				@Html.TextBoxFor( m => m.PalletCost, new { @class = "form-control" } )
				@Html.ValidationMessageFor( m => m.PalletCost, "", new { @class = "text-danger" } )

			</div>
			<div class="col-xs-6">

				@Html.LabelFor( m => m.GravelCost, new { @class = "control-label" } ) <span class="required">Required</span>
				@Html.TextBoxFor( m => m.GravelCost, new { @class = "form-control" } )
				@Html.ValidationMessageFor( m => m.GravelCost, "", new { @class = "text-danger" } )

			</div>
			<div class="col-xs-6">

				@Html.LabelFor( m => m.LaborCost, new { @class = "control-label" } ) <span class="required">Required</span>
				@Html.TextBoxFor( m => m.LaborCost, new { @class = "form-control" } )
				@Html.ValidationMessageFor( m => m.LaborCost, "", new { @class = "text-danger" } )

			</div>
			<div class="col-xs-6">

				@Html.LabelFor( m => m.LandCost, new { @class = "control-label" } ) <span class="required">Required</span>
				@Html.TextBoxFor( m => m.LandCost, new { @class = "form-control" } )
				@Html.ValidationMessageFor( m => m.LandCost, "", new { @class = "text-danger" } )

			</div>
		</div>


</div>

