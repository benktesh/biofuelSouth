            @Html.ValidationMessageFor(m => m.GravelCost)
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.LaborCost, new { @class = "control-label" })
            @Html.TextBoxFor(m => m.LaborCost, new { @class = "form-control" })
            @Html.ValidationMessageFor(m => m.LaborCost)
        </div>
        <div class="form-group">
            @Html.LabelFor(m => m.LandCost, new { @class = "control-label" })
