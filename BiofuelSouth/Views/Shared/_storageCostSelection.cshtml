@using BiofuelSouth.Models
@model BiofuelSouth.Models.Storage


<div class="">
    @Html.LabelFor(m => m.CostOption, new { @class = "control-label" })
    @Html.DropDownListFor(m => m.CostOption, EnumHelper.GetSelectList(typeof(CostEstimationOption)), "Select", new { @class = "form-control" })
    @Html.ValidationMessageFor(m => m.CostOption)
</div>
