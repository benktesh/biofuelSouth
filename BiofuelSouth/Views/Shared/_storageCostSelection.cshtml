    @Html.TextBoxFor(m => m.UserEstimatedCost, new { @class = "form-control" })
    @Html.ValidationMessageFor(m => m.UserEstimatedCost)
</div>
    <div id="UserSupplyMaterialCost">
        <div class="form-group">
            @Html.LabelFor(m => m.TarpCost, new { @class = "control-label" })
